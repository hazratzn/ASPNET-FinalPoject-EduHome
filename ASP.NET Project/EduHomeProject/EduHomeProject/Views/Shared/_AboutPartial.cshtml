@model HomeAbout 

<!-- About Start -->
<div class="about-area pb-155">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-sm-6">
                <div class="about-content">
                    @Html.Raw(Model.Ttile)
                    @Html.Raw(Model.Description)

                    <a class="default-btn" asp-controller="About" asp-action="Index">view courses</a>
                </div>
            </div>
            <div class="col-md-6 col-sm-6">
                <div class="about-img">
                    <img src="~/Assets/img/about/@Model.Image" alt="about">
                </div>
            </div>
        </div>
    </div>
</div>
<!-- About End -->